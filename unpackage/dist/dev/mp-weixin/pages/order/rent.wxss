.container {
  font-size: 24rpx;
  min-height: 100%;
  padding: 0 20px 10px;
  overflow: hidden;
}
.container .goods {
  padding-bottom: 60px;
}
.container .goods .order-list {
  overflow: hidden;
}
.container .goods .order-list .listTop {
  min-height: 75px;
}
.container .goods .order-list .listTop .listTitle {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.container .goods .order-list .listTop .listCon ._span {
  background: rgba(87, 189, 162, 0.1);
  border-radius: 9px;
  color: #57bda2;
  text-align: center;
  padding: 1pt 5pt;
}
.container .goods .order-list .listTop .listCon ._i {
  font-style: normal;
  color: #57bda2;
}
.container .goods .order-list .listTop .rentalsNum {
  background: rgba(250, 126, 18, 0.1);
  border-radius: 8px;
  color: #f6790c;
  line-height: 14px;
  display: inline-block;
  padding: 1pt 5pt;
}
.container .goods .order-list .listTop .tagMjd {
  display: inline-block;
  padding: 2px 8px;
  border-radius: 6px;
  background: #ff8484;
  color: #fff;
}
.container .goods .goods-list-cost {
  overflow: hidden;
  color: #fa7e12;
}
.container .goods .goods-list-cost .goods-item {
  width: 32%;
  background: #fff;
  border-radius: 10px;
  border: 1px solid #e2e2e2;
  margin-left: 2px;
  margin-right: 2px;
  padding: 15px 5px 15px 15px;
  overflow: hidden;
}
.container .goods .goods-list-cost .goods-item ._p {
  font-weight: 600;
  color: #333;
  font-size: 14px;
}
.container .goods .goods-list-cost .goods-item ._span {
  margin-right: 0;
}
.container .goods .goods-list-cost .act {
  background: #fff3e9;
  border: 1px solid #fa7e12;
}
.container .goods .goods-list-cost .no-act {
  background: #f5f5f5;
  color: #999;
}
.container .goods .goods-list-cost ._em {
  font-style: normal;
  font-size: 16px;
  font-weight: 600;
}
.container .goods .duration ._span {
  font-size: 14px;
  font-weight: 600;
  color: #333;
}
.container .goods .duration .mintui-iconless:before {
  content: "-";
}
.container .goods .duration .mintui-tianjia:before {
  content: "+";
}
.container .goods .money-item {
  font-size: 14px;
}
.container .goods .money-item ._span {
  font-weight: 600;
  color: #333;
}
.container .goods .money-item ._strong {
  font-size: 16px;
  color: #fa7e12;
}
.container .goods .notice-line {
  text-align: right;
  font-size: 14px;
  font-weight: 400;
  color: #4ca18c;
  line-height: 20px;
  margin: 20px 0;
}
.container .goods .wrapZsd {
  margin: 20px 0;
  font-size: 14px;
  font-weight: 500;
  color: #333;
  line-height: 20px;
  font-weight: 700;
}
.container .goods .wrapZsd .right {
  color: #999;
}
.container .goods .btn-fixed-bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  padding: 20rpx;
  background: #fafafa !important;
}
.container .goods .btn-fixed-bottom ._span {
  line-height: 44px;
  font-size: 14px;
  font-weight: 700;
}
.container .goods .btn-fixed-bottom ._span ._strong {
  font-size: 16px;
  color: #fa7e12;
}

